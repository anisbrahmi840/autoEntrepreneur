{% extends 'admin.html.twig' %}

{% block title %}Entrepreneur{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="section-title">
            </div>
        </div>
    </div>
    <div class="row d-flex justify-content-center">
        <!-- Contact Form -->
        <div class="col-md-8 col-sm-6 col-xs-12">
            <h1>Entrepreneur</h1>

            <table class="table">
                <tbody>
                    <tr>
                        <th>Id:</th>
                        <td>{{ entrepreneur.id }}</td>
                    </tr>
                    <tr>
                        <th>Nom:</th>
                        <td>{{ entrepreneur.nom }}</td>
                    </tr>
                        <th>Prenom:</th>
                        <td>{{ entrepreneur.prenom }}</td>
                    </tr>
                        <th>Email:</th>
                        <td>{{ entrepreneur.email }}</td>
                    </tr>
                    <tr>
                        <th>Cin:</th>
                        <td>{{ entrepreneur.cin }}</td>
                    </tr>
                    <tr>
                        <th>Password</th>
                        <td>{{ entrepreneur.genre }}</td>
                    </tr>
                    <tr>
                        <th>Date de naissance</th>
                        <td>{{ entrepreneur.datenais | date('Y-m-d') }}</td>
                    </tr>
                    <tr>
                        <th>Ville</th>
                        <td>{{ entrepreneur.villenais }}</td>
                    </tr>
                    <tr>
                        <th>tel</th>
                        <td>{{ entrepreneur.tel }}</td>
                    </tr>
                    <tr>
                        <th>Etat</th>
                        <td>{% if entrepreneur.etat %} Vérifié {% else %} Non vérifié {% endif %}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
{% endblock %}