{% extends 'admin.html.twig' %}

{% block title %}Activite{% endblock %}

{% block body %}

<div class="row justify-content-center">
    <div class="col-md-8 col-sm-12 col-xs-12">
        <h1 class="">Activite</h1>
        <table class="table">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ activite.id }}</td>
                </tr>
                <tr>
                    <th>Nom</th>
                    <td>{{ activite.nom }}</td>
                </tr>
                <tr>
                    <th>Taux</th>
                    <td>{{ activite.taux }}</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
{% endblock %}