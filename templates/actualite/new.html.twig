{% extends 'admin.html.twig' %}

{% block title %}Ajouter Actualité{% endblock %}

{% block body %}
<div class="row justify-content-center">
    <div class="col-md-6 col-sm-12 col-xs-12 shadow p-2">
        <div class="text-center">
            <h1>Ajouter Actualité</h1>
        </div>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <div class="text-center">
            <button class="btn btn-primary">{{ button_label|default('Ajouter Actualité') }}</button>
        </div>
        {{ form_end(form) }}
    </div>
</div>

{% endblock %}