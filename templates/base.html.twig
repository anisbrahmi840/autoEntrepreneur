<html>

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>
		{% block title %}{% endblock %}
	</title>
	<style>
		.page-link{
			Color: #ED1C24
		}
		.page-item.active .page-link {
			background-color: #ED1C24;
			border-color: #ED1C24;
		}
	</style>
	{% block stylesheets %}

	{% endblock %}
	<style>
		nav.navbar {
			padding: 0;
		}
	</style>
	<!-- Favicon -->
	<link rel="icon" type="image/png" href="{{ asset('images/favicon.png') }}">

	<!-- Web Font -->
	<link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
		integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
	<!-- Tromas CSS -->
	<link rel="stylesheet" href="{{ asset('css/theme-plugins.css') }}">
	<link rel="stylesheet" href="{{ asset('css/style.css') }}">
	<link rel="stylesheet" href="{{ asset('css/responsive.css') }}">

	<!-- Tromas Color -->
	<link rel="stylesheet" href="{{ asset('css/skin/skin1.css') }}">

</head>

<body class="box-bg">
	<div class="boxed-layout">
		<!-- Start Header -->
		<header id="header" class="header">
			<div class="container">
				<div class="row">
					<div class="col-md-3 col-sm-3 col-xs-12">
						<!-- Logo -->
						<div class="logo">
							<a href="{{ path('app_home')}}"><img src="{{ asset('images/logo.png') }}" alt="logo"></a>
						</div>
						<!--/ End Logo -->
						<div class="mobile-nav"></div>
					</div>
					<div class="col-md-9 col-sm-9 col-xs-12">
						<!-- Header Widget -->
						<div class="header-widget">
							<!-- Single Widget -->
							<div class="single-widget">
								<i class="fa fa-envelope"></i>
								<h4>Adresse e-mail</h4>
								<p>
									<a href="mailto:info@yourwebsite">info@entrepreneur.com</a>
								</p>
							</div>
							<!--/ End Single Widget -->
							<!-- Single Widget -->
							<div class="single-widget">
								<i class="fa fa-phone"></i>
								<h4>Téléphone</h4>
								<p>+216 70 000 000</p>
							</div>
							<!--/ End Single Widget -->
							{% if not app.user %}
							<div class="single-widget">
								<a href="{{ path('users_login') }}" class="btn radius text-white">Connecter</a>
							</div>
							{% else %}
							<div class="single-widget">							
								<i class="fa fa-user" style="font-size:30px"></i>
								<a class="btn-link " href="{{ path('entrepreneur_show', {slug: app.user.slug}) }}"
									id="dropdownMenuLink" data-toggle="dropdown" 
									aria-expanded="false">
									<p class="mt-2">Mon compte</p>
								</a>
								<div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
									<a class="dropdown-item" href="{{ path('entrepreneur_dashboard', {slug: app.user.slug}) }}#services">Dashboard</a>
									<a class="dropdown-item" href="{{ path('entrepreneur_show', {slug: app.user.slug}) }}">Profile</a>
									<hr>
									<a class="dropdown-item" href="{{ path('app_logout') }}">Déconnexion</a>
								</div>
							</div>
							{% endif %}
						</div>
						<!--/ End Header Widget -->
					</div>
				</div>
			</div>
			<!-- Header Inner -->
			<div class="header-inner">
				<div class="container">
					<div class="row">
						<div class="col-md-12 col-sm-12 col-xs-12">
							<div class="nav-area">
								<!-- Main Menu -->
								<nav class="navbar navbar-expand-lg">
									<div class="">
										<ul class="nav navbar-nav">
											<li class="active">
												<a href="{{ path('app_home') }}"><i class="fa fa-home" style="font-size:24px"></i> </a>
											</li>
											<li class="">
												<a href="{{ path('apropos_index') }}">Qui sommes-nous</a>
											</li>
											<li class="">
												<a href="{{ path('temoignage_index_entrepreneur') }}">Témoignages</a>
											</li>
											<li class="">
												<a href="{{ path('actualite_index_entrepreneur') }}">actualités</a>
											</li>
											<li class="">
												<a href="{{ path('contactus_index') }}">CONTACT</a>
											</li>
										</ul>
									</div>
								</nav>
								<!--/ End Main Menu -->
							</div>
						</div>
					</div>
				</div>
			</div>
			{% block body %}{% endblock %}
			<footer id="footer" class="footer">
				<div class="footer-top">
					<div class="container">
						<div class="row">
							<div class="col-md-4 col-sm-6 col-xs-12">
								<!-- Address Widget -->
								<div class="single-widget address">
									<h2>Nos contacts</h2>
									<ul class="list">
										<li>
											<i class="fa fa-phone"></i> &nbsp; Téléphone: +216 70 000 000
										</li>
										<li>
											<i class="fa fa-envelope"></i>&nbsp; Email:<a
												href="mailto:info@entrepreneur.com">&nbsp; Info@entrepreneur.com</a>
										</li>
										<li>
											<i class="fa fa-map-o"></i> &nbsp; Address: Tunisie, Tunis, 01 rue milaha</li>
									</ul>
								</div>
								<!--/ End Address Widget -->
							</div>
							<div class="col-md-4 col-sm-6 col-xs-12">
								<!-- Links Widget -->
								<div class="single-widget links">
									<h2>Page</h2>
									<ul class="list">
										<li>
											<a href="#"> HOME</a>
										</li>
										<li>
											<a href="{{ path('faq_index') }}"> FAQ</a>
										</li>
										<li>
											<a href="{{ path('temoignage_index_entrepreneur') }}"> TÉMOIGNAGES</a>
										</li>
										<li>
											<a href="{{ path('actualite_index_entrepreneur') }}"> ACTUALITÉS</a>
										</li>
										<li>
											<a href="{{ path('apropos_index') }}"> À PROPOS</a>
										</li>
										<li>
											<a href="{{ path('contactus_index') }}"> CONTACT</a>
										</li>
									</ul>
								</div>
								<!--/ End Links Widget -->
							</div>
							<div class="col-md-4 col-sm-6 col-xs-12">
								<!-- Twitter Widget -->
								<div class="single-widget twitter">
									<h2>Suivez nous</h2>
									<div class="single-tweet">
										<i class="fa fa-facebook"></i><a href="#">Entrepreneur Tunisie</a>
									</div>
									<div class="single-tweet">
										<i class="fa fa-twitter"></i><a href="#">@Entrepreneur_Tunisie</a>
									</div>
									<div class="single-tweet">
										<i class="fa fa-instagram"></i><a href="#">Entrepreneur Tunisie</a>
									</div>
								</div>
								<!--/ End Twitter Widget -->
							</div>
						</div>
					</div>
				</div>
				<div class="footer-bottom">
					<div class="container">
						<div class="row">
							<div class="col-md-12 col-sm-12 col-xs-12">
								<!-- copyright -->
								<div class="copyright">
									<p>&copy; 2021 All Right Reserved. Design & Developed By
										<a href="https://www.sqli-services.com/" target="_blank">
											<u>SQLI Services</u>
										</a>
									</p>
								</div>
								<!--/ End Copyright -->
							</div>
						</div>
					</div>
				</div>
			</footer>
			<script src="{{ asset('js/jquery.min.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/modernizr.min.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/theme-plugins.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/googlemapapi.js') }}" type="text/javascript"></script>
			<script src="{{ asset('js/main.js') }}" type="text/javascript"></script>
			<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
				integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
				crossorigin="anonymous"></script>
			<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
				integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
				crossorigin="anonymous"></script>
			<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
				integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
				crossorigin="anonymous"></script>
			{% block javascripts %}
			{% endblock %}

</body>

</html>